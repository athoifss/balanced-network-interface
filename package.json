  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start:prod": "pnpm run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "pnpm run eslint src",
    "lint:fix": "pnpm run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "prettify": "prettier --write",
    "prei18n:extract": "touch src/locales/en-US.po",
    "i18n:extract": "lingui extract --locale en-US",
    "i18n:compile": "pnpm i18n:extract && lingui compile",
    "i18n:pseudo": "lingui extract --locale pseudo && lingui compile",
    "clean": "rm -rf node_modules"
  },
  "proxy": "https://balanced.sudoblock.io",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm run eslint --max-warnings=0"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "@lingui/cli": "4.7.1",
    "@lingui/vite-plugin": "4.7.1",
    "@types/lodash": "4.14.168",
